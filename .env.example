# .env.example - Rename this file to .env and fill in your actual credentials
# Do NOT commit your .env file to version control.

# Discord Bot Token
DISCORD_BOT_TOKEN="YOUR_DISCORD_BOT_TOKEN_HERE"

# OpenAI API Key
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE"

# Supabase Configuration
SUPABASE_URL="YOUR_SUPABASE_PROJECT_URL_HERE"
SUPABASE_KEY="YOUR_SUPABASE_ANON_PUBLIC_KEY_OR_SERVICE_ROLE_KEY_HERE"

# Optional: Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL="INFO"

# Optional: Admin User IDs (comma-separated, if you want to restrict some commands)
# ADMIN_USER_IDS="YOUR_USER_ID_1,YOUR_USER_ID_2"

# Optional: Channel ID where the bot should post questions and interact
QUIZ_CHANNEL_ID="YOUR_DISCORD_CHANNEL_ID_HERE"